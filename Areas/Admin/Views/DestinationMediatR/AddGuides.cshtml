@model CreateGuideCommand 
@{
    ViewData["Title"] = "AddGuides";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>CQRS MeditR Rehber Ekleme</h1>

<form method="post">
   
    <label>Rehber Adı</label>
    <input type="text" asp-for="GuideName" class="form-control" />
    <br />
    <label>Rehber Açıklaması</label>
    <input type="text" asp-for="Description" class="form-control" />
    <br />
    <button class="btn btn-outline-success">Yeni Rehber Ekle</button>
</form>