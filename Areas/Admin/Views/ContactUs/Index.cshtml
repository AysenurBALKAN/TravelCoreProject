@model List<ContactUs>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Bize Ulaşın Mesajları</h1>

<table class="table table-hovered">
<tr>
    <th>#</th>
    <th>Gönderen</th>
    <th>Konu</th>
    <th>Tarih</th>
    <th>Mesajı Aç</th>
    <th>Sil</th>
    

</tr>
@foreach (var item in Model)
{
<tr>
    <td>@item.ContactUsId</td>
    <td>@item.Name</td>
    <td>@item.Subject</td>
    <td>@item.MessageDate</td>
    <td><a href="#" class="btn btn-outline-info">Siteye Git</a></td>
    <td><a href="#" class="btn btn-outline-danger">Sil</a></td>
</tr>
}
</table>

